<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>

        <General>
            <ROOT>
                <TCEforms>
                    <sheetTitle>
                        LLL:EXT:sessionplaner/Resources/Private/Language/locallang_be.xlf:display_flexform.sheet_general
                    </sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.suggestions>
                        <TCEforms>
                            <label>
                                LLL:EXT:sessionplaner/Resources/Private/Language/locallang_be.xlf:session_flexform.settings.suggestions
                            </label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.suggestions>
                    <settings.listViewHeadline>
                        <TCEforms>
                            <label>
                                LLL:EXT:sessionplaner/Resources/Private/Language/locallang_be.xlf:session_flexform.settings.listViewHeadline
                            </label>
                            <config>
                                <max>256</max>
                                <size>50</size>
                                <softref>typolink_tag</softref>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.listViewHeadline>
                    <settings.listViewText>
                        <TCEforms>
                            <label>
                                LLL:EXT:sessionplaner/Resources/Private/Language/locallang_be.xlf:session_flexform.settings.listViewText
                            </label>
                            <config>
                                <type>text</type>
                                <enableRichtext>1</enableRichtext>
                                <RTE>
                                    <notNewRecords>1</notNewRecords>
                                    <RTEonly>1</RTEonly>
                                    <type>script</type>
                                    <title>
                                        LLL:EXT:frontend/Resources/Private/Language/locallang_ttc.xlf:bodytext.W.RTE
                                    </title>
                                    <icon>actions-wizard-rte</icon>
                                    <module>
                                        <name>wizard_rte</name>
                                    </module>
                                </RTE>
                            </config>
                            <defaultExtras>richtext:rte_transform[flag=rte_enabled|mode=ts_css]</defaultExtras>
                        </TCEforms>
                    </settings.listViewText>
                    <settings.days>
                        <TCEforms>
                            <label>
                                LLL:EXT:sessionplaner/Resources/Private/Language/locallang_be.xlf:session_flexform.settings.days
                            </label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>tx_sessionplaner_domain_model_day</allowed>
                                <autoSizeMax>100</autoSizeMax>
                                <maxitems>100</maxitems>
                                <minitems>0</minitems>
                                <size>2</size>
                                <wizards>
                                    <suggest>
                                        <type>suggest</type>
                                    </suggest>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.days>
                    <settings.singlePid>
                        <TCEforms>
                            <label>
                                LLL:EXT:sessionplaner/Resources/Private/Language/locallang_be.xlf:session_flexform.settings.singlePid
                            </label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                                <size>1</size>
                                <wizards>
                                    <suggest>
                                        <type>suggest</type>
                                    </suggest>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.singlePid>
                </el>
            </ROOT>
        </General>

    </sheets>
</T3DataStructure>
